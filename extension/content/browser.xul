<?xml version="1.0"?>
<?xml-stylesheet href="chrome://jetpack/content/css/browser.css" type="text/css"?>
<?xml-stylesheet href="chrome://jetpack/skin/browser.css" type="text/css"?>
<overlay xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script type="application/javascript;version=1.7"
          src="chrome://jetpack/content/js/xul/browser.js"/>
  <script type="application/javascript;version=1.7"
          src="chrome://jetpack/content/js/jetpack-app-nuances.js"/>
  <menupopup id="taskPopup">
    <menuitem id="jetpack-safe" insertafter="javascriptConsole"
              label="about:jetpack (Safe Mode)"
              oncommand="gJetpack.openAboutPage(true);"/>
    <menuitem id="jetpack" insertafter="javascriptConsole"
              label="about:jetpack" accesskey="t"
              oncommand="gJetpack.openAboutPage(false);"/>
  </menupopup>
  <menupopup id="menu_ToolsPopup">
    <menuitem id="jetpack-safe" insertafter="javascriptConsole"
              label="about:jetpack (Safe Mode)"
              oncommand="gJetpack.openAboutPage(true);"/>
    <menuitem id="jetpack" insertafter="javascriptConsole"
              label="about:jetpack" accesskey="t"
              oncommand="gJetpack.openAboutPage(false);"/>
  </menupopup>
</overlay>
